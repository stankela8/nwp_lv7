<h2>Novi projekt</h2>

<form action="/projects" method="POST">

    <div class="mb-3">
        <label>Naziv</label>
        <input type="text" name="naziv" class="form-control">
    </div>

    <div class="mb-3">
        <label>Opis</label>
        <textarea name="opis" class="form-control"></textarea>
    </div>

    <div class="mb-3">
        <label>Cijena</label>
        <input type="number" name="cijena" class="form-control">
    </div>

    <div class="mb-3">
        <label>Obavljeni poslovi</label>
        <input type="text" name="obavljeniPoslovi" class="form-control">
    </div>

    <div class="mb-3">
        <label>Datum početka</label>
        <input type="date" name="datumPocetka" class="form-control">
    </div>

    <div class="mb-3">
        <label>Datum završetka</label>
        <input type="date" name="datumZavrsetka" class="form-control">
    </div>

    <button class="btn btn-success">Spremi</button>
</form>
